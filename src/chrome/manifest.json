{
  "manifest_version": 2,
  "name": "Smart TOC",
  "version": "0.2.6",
  "description": "Add a table of contents for article you are reading",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Toggle table of contents"
  },
  "icons": {
    "16": "icon.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "history.js",
      "background.js"
    ],
    "persistent": false
  },
  "commands": {
    "toggle": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E",
        "chromeos": "Ctrl+Shift+E",
        "linux": "Ctrl+Shift+E"
      },
      "description": "Show/hide TOC"
    },
    "prev": {
      "description": "Jump to previous heading"
    },
    "next": {
      "description": "Jump to next heading"
    }
  },
  "permissions": [
    "activeTab",
    "notifications"
  ],
  "author": "FallenMax@gmail.com"
}
